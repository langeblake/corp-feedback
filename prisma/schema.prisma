generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  id          Int      @id @default(autoincrement())
  text        String
  upvoteCount Int      @default(0)
  company     String
  badgeLetter String
  createdAt   DateTime @default(now())
}
